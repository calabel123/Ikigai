		except TypeError: # if NAOqi < 1.14
		  GeneratedClass.__init__( self )

	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("tell me more")):
			self.output_1(p)
		elif(p == self.typeConversion("continue")):
			self.output_2(p)
		elif(p == self.typeConversion("next")):
			self.output_3(p)
		elif(p == self.typeConversion("yes")):
			self.output_4(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
